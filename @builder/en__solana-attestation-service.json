                "component": {
                  "name": "Copy",
                  "options": {
                    "rawHtml": {
                      "@type": "@builder.io/core:LocalizedValue",
                      "Default": "<p>This is only a small snapshot of what’s possible. With SAS, you can attest to anything — and developers can focus on building better user experiences while relying on the open SAS standard.</p>"
                    }
                  }
                },
                "responsiveStyles": {
                  "large": {
                    "display": "flex",
                    "flexDirection": "column",
