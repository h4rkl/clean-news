              },
              {
                "@type": "@builder.io/sdk:Element",
                "@version": 2,
                "id": "builder-afeb05741284415d9bb443f75c40d8c6",
                "meta": {
                  "previousId": "builder-6cc7cb08cae14e1990f2a3b1b74871b8"
                },
                "component": {
                  "name": "Youtube",
                  "options": {
                    "url": "https://youtu.be/3ZMEQqyj6aM"
                  }
                },
                "responsiveStyles": {
                  "large": {
