                        "component": {
                          "name": "Copy",
                          "options": {
                            "rawHtml": "<h3><strong>Sleepr</strong></h3><p>Sleepr demonstrated how anonymity can foster deeper connection, inviting attendees to speak to him 1:1 on the phone. Over 500 patrons took part in these private phone conversations.</p>"
                          }
                        },
