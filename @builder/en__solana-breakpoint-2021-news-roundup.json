            "component": {
              "name": "Text",
              "options": {
                "text": "<p>https://twitter.com/rajgokal/status/1460628552475299841?ref_src=twsrc%5Etfw%7Ctwcamp%5Etweetembed%7Ctwterm%5E1460628552475299841%7Ctwgr%5E4e3fed4d8f9342e2979d1acf935fefe33a886ad6%7Ctwcon%5Es1_&amp;ref_url=https%3A%2F%2Fsolana.com%2Fnews%2Fsolana-breakpoint-2021-news-roundup</p>"
