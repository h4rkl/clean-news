          {
            "@type": "@builder.io/sdk:Element",
            "@version": 2,
            "id": "builder-8646db95f61d41508cff4b1bc8437c7b",
            "component": {
              "name": "Custom Code",
              "options": {
                "code": "<iframe width=\"100%\" height=\"55%\" src=\"https://www.youtube.com/embed/pk66JHNMfVY\" title=\"YouTube video player\" frameborder=\"0\" allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen=\"\"></iframe>",
                "scriptsClientOnly": true
              }
            },
            "responsiveStyles": {
              "large": {
                "display": "flex",
                "flexDirection": "column",
                "position": "relative",
