            "component": {
              "name": "Copy",
              "options": {
                "rawHtml": "<p>Breakpoint 2023 took place in Amsterdam from October 30 - November 3, bringing together over 3,000 developers, investors, traders, enterprise leaders, vendors, artists, musicians, and crypto-curious minds. Sessions covered innovation in the ecosystem and future plans for the network.&nbsp;   </p><p><br></p>"
              }
            },
            "responsiveStyles": {
