              "options": {
                "code": "<iframe src=\"https://giphy.com/embed/12Nv3nBSCAbLO0/twitter/iframe\" width=\"435\" height=\"261\" frameborder=\"0\" scrolling=\"no\"></iframe>",
                "scriptsClientOnly": true
