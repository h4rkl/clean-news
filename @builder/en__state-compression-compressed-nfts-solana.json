          {
            "@type": "@builder.io/sdk:Element",
            "@version": 2,
            "id": "builder-80b3a0c5e9594a69aafb184e24dac897",
            "component": {
              "name": "Copy",
              "options": {
                "rawHtml": "<p>\n  <em\n    >This chart is based on a snapshot taken on April 5, 2023 and based on a\n    price of SOL at $21.14, MATIC at $1.14, and ETH at $1,909.45. </em\n  ><a\n    href=\"https://solana.com/news/state-compression-compressed-nfts-solana#footnotes\"\n    rel=\"noopener noreferrer\"\n    target=\"_blank\"\n    ><em>See footnote.</em></a\n  >\n</p>\n"
              }
            },
            "responsiveStyles": {
              "large": {
