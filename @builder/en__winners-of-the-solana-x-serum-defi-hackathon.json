          {
            "@type": "@builder.io/sdk:Element",
            "@version": 2,
            "id": "builder-3edeeab1048b4ce6b7f4e73063455ad1",
            "component": {
              "name": "Text",
              "options": {
                "text": "<p><span style=\"font-size: 20px;\">To accelerate the already rapid growth of the ecosystem, the Solana Foundation recently teamed up with Serum, a leading project in Solana’s ecosystem, to host a virtual&nbsp;</span><a href=\"https://solana.com/defi?ref=solana.ghost.io\" rel=\"noopener noreferrer\" target=\"_blank\" style=\"font-size: 20px;\">DeFi Hackathon</a><span style=\"font-size: 20px;\">. The event brought together crypto pioneers, popular DeFi projects, and builders from around the world for two weeks of hacking on Solana and Serum.</span></p>"
              }
            },
